(let ([tabulate (lambda (op)
                  (begin (print (op 1 4))
                  (begin (print (op 4 9))
                  (begin (print (op 9 16))
                         (print (op 16 25))))))])
  (begin (tabulate +)
  (begin (tabulate -)
  (begin (tabulate *)
  (begin (tabulate <)
  (begin (tabulate <=)
         (tabulate =)))))))
