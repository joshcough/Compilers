(((eax <- (allocate 21 3))
  (esi <- eax)
  (edi <- 1)
  :loop
  (eax <- (print esi))
  (eax <- edi)
  (eax *= 4)
  (eax += esi)
  (ecx <- (mem eax 0))
  (eax += 4)
  (edx <- (mem eax 0))
  (edx >>= 1)
  (ecx >>= 1)
  (edx += ecx)
  (edx <<= 1)
  (edx += 1)
  ((mem eax 0) <- edx)
  (edi += 1)
  (cjump edi < 10 :loop :quit)
  :quit))
