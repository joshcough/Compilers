  ; put something on the stack
  ((mem ebp -4) <- 3)
  ((mem ebp -8) <- 85)
  ((mem ebp -12) <- 109)

  (ebx <- (mem esp 0))
  (eax <- (print ebx))

  (esp += 4)
  (ebx <- (mem esp 0))
  (eax <- (print ebx))

  (esp += 4)
  (ebx <- (mem esp 0))
  (eax <- (print ebx))

