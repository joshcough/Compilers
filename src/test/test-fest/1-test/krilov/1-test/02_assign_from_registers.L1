; Test assigning from one register to another
((
  
  (ecx <- 11)
  (edx <- ecx)
  (eax <- (print edx))   ; 5
  
  ; Test assigning from the same register
  (ecx <- 13)    ; pick a different number
  (ecx <- ecx)   ; assign from itself - value shouldn't change
  (eax <- (print ecx))   ; 6
))